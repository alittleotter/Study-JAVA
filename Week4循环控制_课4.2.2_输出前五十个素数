package hi;
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int count=0;
		for(int j=2;j<100000;j++)
		{
			if(count<50)
			{	
				int isPrime=1;
				for(int i=2;i<j;i++)
				{
					if(j%i==0)
					{
						isPrime=0;
						break;
					}
				}	
				if(isPrime==1)
				{
					System.out.println(j+" ");
					count++;
				}
				else{	}
			}	
		}
     }
}
